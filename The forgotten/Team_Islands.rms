
/* Team Islands for Forgotten Empires */

/* Note: random_map.def is automatically included in every rms script */

<PLAYER_SETUP>
  random_placement 
	ai_info_map_type TEAM_ISLANDS 0 0 0

<LAND_GENERATION>
base_terrain WATER

start_random
  percent_chance 20
  #define DESERT_MAP
  percent_chance 20
  #define ALPINE_MAP
  percent_chance 20
  #define FROZEN_MAP
end_random

create_player_lands 
{            
if DESERT_MAP
  terrain_type                     DIRT
elseif ALPINE_MAP
  terrain_type                     GRASS2
elseif FROZEN_MAP
  terrain_type                     SNOW
else           
  terrain_type                     GRASS
endif
land_percent                     45
base_size                        12
border_fuzziness                 15
start_random
   percent_chance                  50
      left_border                   7
      right_border                  9
   percent_chance                  50
	left_border                   9
      right_border                  7
end_random
start_random
   percent_chance                  50
      top_border                    7
      bottom_border                 9
   percent_chance                  50
	top_border                    9
      bottom_border                 7
end_random
  set_zone_by_team
  other_zone_avoidance_distance    22
}

